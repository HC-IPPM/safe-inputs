{
  "extends": ["../.eslintrc", "plugin:n/recommended"],
  "env": {
    "node": true,
  },
  "settings": {
    "import/resolver": {
      "node": {
        "moduleDirectory": ["./", "./node_modules/"],
      },
    },
  },
  "rules": {
    "n/no-unsupported-features/es-syntax": [
      "error",
      { "ignores": ["modules"] },
    ],
    "n/no-missing-import": 0, // https://github.com/mysticatea/eslint-plugin-node/issues/248
    "no-console": 0,
  },
}
