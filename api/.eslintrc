{
  "extends": ["../.eslintrc", "plugin:node/recommended"],
  "plugins": ["node"],
  "env": {
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": "2022",
    "sourceType": "module",
    "allowImportExportEverywhere": true
  },
  "rules": {
    "node/no-unsupported-features/es-syntax": [
      "error",
      { "ignores": ["modules"] }
    ],
    "no-console": 0
  }
}
